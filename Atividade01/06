#include <stdio.h>
#include <stlib.h>

void MEDIA(float a, float b, float c , float d){
   float result;     
   result = (a+b+c+d)/4;
   printf("A MEDIA  Ã© %f.02",Result);
   break;
}

int main(){
    int num1,num2,num3,num4;

   	scanf("%d", &num1);
   	scanf("%d", &num2);
   	scanf("%d", &num3);
   	scanf("%d", &num4);

   	MEDIA(num1,num2,num3,num4);
    
	return 0;

}

